    });
    $('#myModal').data('bs.modal').$backdrop.css('background-color', 'orange');
    function modalfunction() {
        var modalcategories = "";
        if (document.getElementById("scales-1").checked) { modalcategories = modalcategories.concat(document.getElementById("scales-1").value, ","); }
        if (document.getElementById("scales-2").checked) { modalcategories = modalcategories.concat(document.getElementById("scales-2").value, ","); }
        if (document.getElementById("scales-3").checked) { modalcategories = modalcategories.concat(document.getElementById("scales-3").value, ","); }
        if (document.getElementById("scales-4").checked) { modalcategories = modalcategories.concat(document.getElementById("scales-4").value, ","); }
        if (document.getElementById("scales-5").checked) { modalcategories = modalcategories.concat(document.getElementById("scales-5").value); }
        var modalarray = modalcategories.split(',');
        var modalcategories1 = "";
        for (var i = 0; i < modalarray.length; i++) {
            if (modalarray[i] != "" && i != modalarray.length - 1) {
                modalcategories1 = modalcategories1 + "" + modalarray[i] + ",";
            }
            if (modalarray[i] != "" && i == modalarray.length - 1) {
                modalcategories1 = modalcategories1 + "" + modalarray[i];
                console.log(modalarray[i]);
            }
        }
        modalcategories1 = modalcategories1.replace(/,\s*$/, "");
        console.log(modalcategories1);
        sessionStorage.setItem('modalstuff', modalcategories1);
        console.log(sessionStorage.getItem('modalstuff'));
        sessionStorage.setItem('popupshown', 'yes');
        $('#myModal').modal('hide');
        var script = document.createElement('script');
        script.type = 'text/javascript';
        script.src = '../../Scripts/events.js';
        document.getElementsByTagName('head')[0].appendChild(script);

        //modalcategories = modalcategories.concat(document.getElementById("scales-1"), ",", document.getElementById("scales-2"), ",", document.getElementById("scales-3"), ",", document.getElementById("scales-4"), ",", document.getElementById("scales-5"));
        //$('#part_number-modal').on('hidden.bs.modal', function (e) {
        //    // do something...
        //})

    }

    function closefunction() {
        sessionStorage.setItem('popupshown', 'yes');
        $('#myModal').modal('hide');
        var script = document.createElement('script');
        script.type = 'text/javascript';
        script.src = '../../Scripts/events.js';
        document.getElementsByTagName('head')[0].appendChild(script);
    }

</script>
<div class="modal fade" class="colorFor" id="myModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" data-backdrop="static">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="exampleModalLabel">Choose Your preferred categories. Best Events from these categories will be shown to you.</h4>
            </div>
            <div class="modal-body">
                <form>
                    <div class="form-group">
                        <input type="checkbox" id="scales-1" name="scales" value="115"
                               checked>
                        <label for="scales">Family & Education</label>
                    </div>

                    <div class="form-group">
                        <input type="checkbox" id="scales-2" name="scales" value="104">
                        <label for="scales">Film & Media</label>
                    </div>

                    <div class="form-group">
                        <input type="checkbox" id="scales-3" name="scales" value="120">
                        <label for="scales">School Activities</label>
                    </div>


                    <div class="form-group">
                        <input type="checkbox" id="scales-4" name="scales" value="108">
                        <label for="scales">Sports & Fitness</label>
                    </div>


                    <div class="form-group">
                        <input type="checkbox" id="scales-5" name="scales" value="109">
                        <label for="scales">Travel &amp; Outdoor</label>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-success" data-dismiss="modal" onclick="closefunction()">Close</button>
                <button type="button" class="btn btn-success" onclick="modalfunction()">Submit</button>
            </div>
        </div>
    </div>
</div>



