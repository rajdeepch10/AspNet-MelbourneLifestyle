<script>
    $("button").click(function () {
        $("button").removeClass("active");
        $(this).addClass("active");
    });

    var slideIndex = 1;
    showDivs(slideIndex);
    var count = 24;

    function plusDivs(n) {
        showDivs(slideIndex += n);
    }

    function currentDiv(n) {
        showDivs(slideIndex = n);
        $("#test1").text("0/" + count);
    }

    function showDivs(n) {
        var i;
        var x = document.getElementsByClassName("mySlides");
        var dots = document.getElementsByClassName("demo");
        if (n > x.length) { slideIndex = 1 }
        if (n < 1) { slideIndex = x.length }
        for (i = 0; i < x.length; i++) {

            x[i].style.display = "none";
        }
        for (i = 0; i < dots.length; i++) {
            dots[i].className = dots[i].className.replace(" w3-red", "");
        }
        x[slideIndex - 1].style.display = "block";

        dots[slideIndex - 1].className += " w3-red";

        if (n == 1) { count = 24; }
        else if (n == 2) {
            count = 18; document.getElementById("testaa").scrollIntoView();
        }
        else {
            count = 4; document.getElementById("testaa").scrollIntoView();
        }
        $("#test1").text("0/" + count);
        $("input#defaultCheck1:checked").prop('checked', false)
    }

    $(document).ready(function () {

        // get box count
        var checked = 0;

        // count checks
        function countChecked() {
            checked = $("input#defaultCheck1:checked").length;
            var percentage = checked + "/" + count;
            $("#test1").text(percentage);
        }

        countChecked();
        $(":checkbox").click(countChecked);
    });

    $('#print').click(function () {
        const filename = 'Checklist.pdf';
        var w = document.getElementById("content").offsetWidth;
        var h = document.getElementById("content").offsetHeight;
        html2canvas(document.getElementById("content"),
            { scale: 1 }).then(canvas => {
                var filename1 = '';
                if (slideIndex == 1) { filename1 = 'Before arriving Checklist.pdf'; }
                else if (slideIndex == 2) { filename1 = 'After arriving Checklist'; }
                else { filename1 = 'Enjoying life in Melbourne Checklist.pdf'; }
                let pdf = new jsPDF('p', 'mm', 'a4');
                pdf.addImage(canvas.toDataURL('image/png'), 'PNG', 0, 0, 211, 298);
                pdf.save(filename1);
            });
    });
</script>